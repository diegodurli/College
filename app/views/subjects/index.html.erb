
<%= render 'subjects_chart' %>

<div id='subjects_chart'></div><br/><br/>

<table class="table table-striped table-bordered table-condensed">
	<thead>
    <tr>
			<th>Completed</th>
      <th>Pending</th>
			<th>Registered</th>
    </tr>
  </thead>
  <tbody id="subjects_count_by_status">
		<tr>
      <%= render 'subjects_count_by_status' %>
		</tr>
	</tbody>
</table>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Status</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody class="subjects">
    <%= render @subjects %>
  </tbody>
</table>

<%= link_to 'New', new_subject_path, class: 'btn btn-primary btn-large btn-large3' %></br></br><br/>